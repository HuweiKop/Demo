@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title></title>
    <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=8" />
    <!-- stylesheets -->
    <link rel="stylesheet" type="text/css" href="~/resources/css/reset.css" />
    <link rel="stylesheet" type="text/css" href="~/resources/css/iframe_style.css" />
    <link id="color" rel="stylesheet" type="text/css" href="~/resources/css/colors/blue.css" />
    <link rel="stylesheet" type="text/css" href="~/resources/css/rightpage.css" />
    <link rel="StyleSheet" href="~/resources/css/tabletree4j.css" type="text/css" />

    <!-- scripts (jquery) -->
    <script src="@Url.Content("~/resources/scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/resources/scripts/jquery-ui-1.8.24.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/resources/scripts/jquery.ui.selectmenu.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/resources/scripts/tiny_mce/jquery.tinymce.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/resources/scripts/TableTree4J.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/resources/scripts/smooth.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/resources/scripts/smooth.dialog.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/resources/scripts/common.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/Account/ResourceList.js")" type="text/javascript"></script>

    <script type="text/javascript">
        $(function () {
            var resourceString = '@ViewData["resource"]';
            resourceString = resourceString.replace(/&quot;/g, '"');
            var resourceArray = $.parseJSON(resourceString);

            var alreadyResourceString = '@ViewData["alreadyResource"]';
            alreadyResourceString = alreadyResourceString.replace(/&quot;/g, '"');
            var alreadyResourceArray = $.parseJSON(alreadyResourceString);

            showGridTree(resourceArray, alreadyResourceArray)
        })
    </script>
</head>
<body>
    <input type="hidden" id="roleId" value="@ViewData["roleId"]" />

    <div id="content" style="overflow: visible">
        <div id="right" style="overflow: visible">
            <!-- table -->
            <div class="box" style="overflow: visible">
                <!-- box / title -->
                <div class="title">
                    <h5>页面管理列表</h5>
                </div>
                <div id="treeDiv"></div>
                <div class="form">
                        <div class="fields">
                            <div class="buttons" style="width: 100%; margin-left: 0; text-align: center">
                                <button type="button" class="button" onclick="Save()" style="width: 67px">保存</button>
                                <button type="button" class="button" onclick="GoBack()" style="width: 67px">返回</button>
                            </div>
                        </div>
                    </div>
            </div>
            
            <!-- end table -->
        </div>
    </div>
</body>
</html>
